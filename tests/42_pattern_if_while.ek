let pair = [1, 2];
if (match [a, b] = pair if a < b) {
  print("if", a, b);
} else {
  print("if", "no");
}

pair = [2, 1];
if (match [a, b] = pair if a < b) {
  print("if", a, b);
} else {
  print("if", "no");
}

let value = [1];
if (match [a, b] = value if a < b) {
  print("if", a, b);
} else {
  print("if", "no");
}

let loopPair = [1, 2];
while (match [a, b] = loopPair if a < b) {
  print("while", a, b);
  loopPair = [b, a];
}

try {
  let [x, 2] = [1, 3];
