interface Greeter {
  fun greet(name: string): string;
}

class ConsoleGreeter implements Greeter {
  fun greet(name: string): string { return "Hello " + name; }
}

fun makeGreeter() {
  return ConsoleGreeter();
}

let c = di.container();
di.bind(c, "greeter", makeGreeter);
let g = di.resolve(c, "greeter");
print(g.greet("Erkao"));

di.value(c, "answer", 42);
print(di.resolve(c, "answer"));
