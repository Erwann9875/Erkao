fun getUsers(req) {
  return {
    status: 200,
    body: json.stringify([
      { id: 1, name: "Alice" },
      { id: 2, name: "Bob" }
    ]),
    headers: { "Content-Type": "application/json" }
  };
}

fun postUser(req) {
  print("Creating user:", req["body"]);
  return {
    status: 201,
    body: json.stringify({ created: true }),
    headers: { "Content-Type": "application/json" }
  };
}

let apiRoutes = {
  "GET /api/users": getUsers,
  "POST /api/users": postUser
};
